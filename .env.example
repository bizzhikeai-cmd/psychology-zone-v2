# Psychology Zone - Environment Variables
# Copy this file to .env and fill in your actual values

# ===========================================
# RAZORPAY PAYMENT GATEWAY
# ===========================================
# Get from: https://dashboard.razorpay.com/app/keys
# Use test keys for development (rzp_test_xxx)
# Use live keys for production (rzp_live_xxx)
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxx
RAZORPAY_KEY_SECRET=xxxxxxxxxxxxxxxxxxxxxxxx

# ===========================================
# SUPABASE DATABASE
# ===========================================
# Get from: https://app.supabase.com/project/YOUR_PROJECT/settings/api
SUPABASE_URL=https://xxxxxxxxxxxx.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxxxx
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxxxx

# ===========================================
# INTERAKT WHATSAPP API
# ===========================================
# Get from: https://app.interakt.ai/settings/api-keys
INTERAKT_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxx

# WhatsApp message template names (must be approved in Interakt)
INTERAKT_CUSTOMER_TEMPLATE=appointment_confirmation
INTERAKT_ADMIN_TEMPLATE=new_booking_admin

# Admin WhatsApp number for notifications (with country code, no +)
ADMIN_WHATSAPP_NUMBER=918968900002

# ===========================================
# GOOGLE TAG MANAGER
# ===========================================
# Get from: https://tagmanager.google.com
# Format: GTM-XXXXXXX
GTM_CONTAINER_ID=GTM-XXXXXXX

# ===========================================
# ADMIN DASHBOARD
# ===========================================
# Password for admin login at /admin/login
ADMIN_PASSWORD=your_secure_admin_password_here

# Admin email for booking notifications
ADMIN_EMAIL=admin@psychologyzone.in

# Session token (auto-generated, leave empty)
ADMIN_SESSION_TOKEN=

# ===========================================
# EMAIL NOTIFICATIONS (RESEND)
# ===========================================
# Get from: https://resend.com/api-keys
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ===========================================
# SITE CONFIGURATION
# ===========================================
# Your production domain (no trailing slash)
SITE_URL=https://psychologyzone.in
